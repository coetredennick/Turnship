# Gmail API Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:3001/auth/google/callback

# Development Gmail Tokens (get these from setup-dev-tokens.js)
DEV_ACCESS_TOKEN=your_dev_access_token_here
DEV_REFRESH_TOKEN=your_dev_refresh_token_here
DEV_TOKEN_EXPIRY=your_token_expiry_timestamp_here

# OpenAI API Key (for email generation)
OPENAI_API_KEY=your_openai_api_key_here

# Database Configuration
DATABASE_URL=sqlite:./server/dev.db

# Server Configuration
PORT=3001
NODE_ENV=development
CLIENT_URL=http://localhost:5173
